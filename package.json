{
  "name": "fuzzy-searchbox",
  "private": false,
  "version": "1.1.5",
  "description": "An accessible, screen reader friendly combobox with fuzzy searching",
  "author": "Gregory Zhenin",
  "main": "dist/fuzzy-searchbox.umd.js",
  "module": "dist/fuzzy-searchbox.esm.js",
  "unpkg": "dist/fuzzy-searchbox.min.js",
  "browser": {
    "./sfc": "src/fuzzy-searchbox.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "fuse.js": "^3.4.4",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  },
  "keywords": [
    "vue",
    "searchbox",
    "search",
    "dropdown",
    "accessible",
    "screen reader",
    "fuzzy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GregOSP/fuzzy-searchbox.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GregOSP/fuzzy-searchbox/issues"
  },
  "homepage": "https://github.com/GregOSP/fuzzy-searchbox#readme"
}
